{
    "defaultSeverity": "error",
    "extends": [],
    "jsRules": {},
    "rules": {
        "indent": [true, "spaces", 4],
        "no-default-export": true,
        "max-line-length": [true, 100],
        "prefer-const": true,
        "no-var-keyword": true,
        "triple-equals": true,
        "typeof-compare": true,
        "eofline": true,
        "cyclomatic-complexity": true,
        "linebreak-style": [true, "LF"],
        "no-duplicate-imports": true,
        "array-type": [true, "array"],
        "class-name": true,
        "interface-name": false,
        "no-trailing-whitespace": [true, "ignore-comments"],
        "quotemark": [true, "double"],
        "semicolon": [true, "always"],
        "space-before-function-paren": [true, {
            "anonymous": "never", "named": "never", "asyncArrow": "always"
        }],
        "space-within-parens": [true, 0],
        "prefer-template": true,
        "variable-name": [
            true, "ban-keywords", "check-format", "allow-leading-underscore",
            "allow-pascal-case"
        ],
        "whitespace": [true,
            "check-branch", "check-operator", "check-typecast", "check-decl",
            "check-module", "check-separator", "check-rest-spread", "check-type-operator",
            "check-preblock", "check-type"
        ],
        "curly": [true, "ignore-same-line"],
        "object-curly-spacing": [true, "always"],
        "ban-comma-operator": true,
        "prefer-for-of": true,
        "typedef": [false],
        "no-switch-case-fall-through": true,
        "no-duplicate-super": true,
        "no-duplicate-switch-case": true,
        "no-eval": true,
        "no-for-in-array": true,
        "no-implicit-dependencies": [true, ["vue", "views", "assets", "css", "public"]],
        "no-null-keyword": true,
        "no-return-await": true,
        "use-isnan": true,
        "deprecation": true,
        "prefer-readonly": true,
        "arrow-return-shorthand": true,
        "binary-expression-operand-order": true,
        "file-name-casing": [true, "kebab-case"],
        "import-spacing": true,
        "jsdoc-format": true,
        "no-boolean-literal-compare": true,
        "no-irregular-whitespace": true,
        "object-literal-shorthand": true,
        "prefer-method-signature": true,
        "prefer-while": true,
        "no-reference": true,
        "typedef-whitespace": [true,
            {
              "call-signature": "nospace",
              "index-signature": "nospace",
              "parameter": "nospace",
              "property-declaration": "nospace",
              "variable-declaration": "nospace"
            },
            {
              "call-signature": "onespace",
              "index-signature": "onespace",
              "parameter": "onespace",
              "property-declaration": "onespace",
              "variable-declaration": "onespace"
            }
        ],
        "await-promise": true,
        "no-bitwise": true,
        "no-arg": true,
        "no-conditional-assignment": true,
        "no-console": [true, "log", "error"]
    },
    "rulesDirectory": []
}